{
  "name": "blogql-client",
  "version": "1.0.0",
  "description": "BlogQL web interface",
  "main": "index.js",
  "author": {
    "name": "Dave Johnson"
  },
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --mode development",
    "compile": "tsc --noEmit false && yarn run gentypes",
    "test": "jest --detectOpenHandles",
    "build": "yarn compile; webpack",
    "restart": "yarn compile; yarn start",
    "gentypes": "npx apollo codegen:generate --excludes=src/*/*.ts --localSchemaFile=../server/schema.graphql --target=typescript --tagName=gql",
    "gentypes-against-server": "npx apollo codegen:generate --excludes=src/*/*.ts --endpoint=http://localhost:4000/graphql --target=typescript --tagName=gql",
    "clean": "rm -rf node_modules build src/*.js src/*/*.js src/*.jsx src/*/*.jsx __generated__/* src/graphql/__generated__/* schema.graphql",
    "build-storybook": "build-storybook",
    "storybook": "start-storybook -p 6006",
    "nukeit": "yarn clean; rm yarn.lock; yarn install; yarn build"
  },
  "dependencies": {
    "@apollo/client": "^3.7.4",
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "bootstrap": "4.6.0",
    "chromatic": "^6.14.0",
    "react": "^17.0.2",
    "react-bootstrap": "^1.6.6",
    "react-dom": "^17.0.2",
    "react-google-login": "^5.2.2",
    "react-quill": "2.0.0",
    "react-router": "^6.6.2",
    "react-router-bootstrap": "^0.26.2",
    "react-router-dom": "^6.6.2",
    "stack-utils": "^2.0.6",
    "string-strip-html": "^13.0.6",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@graphql-codegen/cli": "^2.6.2",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-interactions": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/builder-webpack5": "^6.5.15",
    "@storybook/manager-webpack5": "^6.5.15",
    "@storybook/react": "^6.5.15",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "~12.1.4",
    "@testing-library/user-event": "^13.1.9",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^16.18.11",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "@types/react-router-bootstrap": "^0.24.5",
    "@types/react-router-dom": "^5.1.7",
    "apollo": "^2.34.0",
    "babel-loader": "^8.3.0",
    "browserify-zlib": "^0.2.0",
    "cross-fetch": "^3.1.5",
    "css-loader": "^6.7.3",
    "expect": "^29.3.1",
    "graphql": "~15.8.0",
    "https-browserify": "^1.0.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "mini-css-extract-plugin": "^2.7.2",
    "msw": "^0.49.2",
    "msw-storybook-addon": "^1.6.3",
    "stream-http": "^3.2.0",
    "timers-browserify": "^2.0.12",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.2",
    "typescript": "~4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "jsdom",
    "testRegex": "(/tests/.tsx|(\\.|/)(test|spec))\\.(tsx?)$",
    "transformIgnorePatterns": [
      "node_modules/?!@apollo"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  },
  "readme": "ERROR: No README data found!",
  "_id": "blogql-client@1.0.0"
}
