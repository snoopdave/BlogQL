{
  "name": "blogql-client",
  "version": "0.0.2",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.5.6",
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^13.1.9",
    "@types/query-string": "^6.3.0",
    "bootstrap": "4.6.0",
    "dotenv": "^10.0.0",
    "react": "^17.0.2",
    "react-bootstrap": "^1.6.0",
    "react-dom": "^17.0.2",
    "react-google-login": "^5.2.2",
    "react-quill": "2.0.0-beta.4",
    "react-router-bootstrap": "^0.25.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "typescript": "~4.2.4",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "tsc && yarn run gentypes && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "gentypes": "npx apollo codegen:generate --excludes=src/*/*.ts --endpoint=http://localhost:4000/graphql --target=typescript --tagName=gql",
    "gentypes-local": "npx apollo codegen:generate --excludes=src/*/*.ts --localSchemaFile=schema.graphql --target=typescript --tagName=gql",
    "clean": "rm -rf node_modules build src/*.js src/*.jsx __generated__/* src/graphql/__generated__/* schema.graphql"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@graphql-codegen/cli": "^2.6.2",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.2",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.7",
    "apollo": "^2.33.9",
    "graphql": "~15.8.0"
  }
}
