{
  "name": "client2",
  "version": "0.1.0",
  "private": true,
  "author": "snoopdave@gmail.com",
  "license": "Apache-2.0",
  "main": "./src/index.js",
  "scripts": {
    "start": "react-scripts start",
    "compile": "tsc --noEmit false && yarn run gentypes",
    "build": "yarn run compile && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "gentypes": "npx apollo codegen:generate --excludes=src/*/*.ts --localSchemaFile=../server/schema.graphql --target=typescript --tagName=gql",
    "gentypes-against-server": "npx apollo codegen:generate --excludes=src/*/*.ts --endpoint=http://localhost:4000/graphql --target=typescript --tagName=gql",
    "clean": "rm -rf node_modules build src/*.js src/*.jsx __generated__/* src/graphql/__generated__/* schema.graphql",
    "build-storybook": "build-storybook -s public",
    "storybook": "start-storybook -p 6006 -s public"
  },
  "dependencies": {
    "@apollo/client": "^3.5.6",
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@storybook/addon-jest": "^6.5.15",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.11",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/react-router-dom": "^5.3.3",
    "bootstrap": "4.6.0",
    "https-browserify": "^1.0.0",
    "react": "^18.2.0",
    "react-bootstrap": "^1.6.6",
    "react-dom": "^18.2.0",
    "react-google-login": "^5.2.2",
    "react-quill": "2.0.0",
    "react-router-bootstrap": "^0.25.0",
    "react-router-dom": "^5.3.3",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.4",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@graphql-codegen/cli": "^2.6.2",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-interactions": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/builder-webpack5": "^6.5.15",
    "@storybook/manager-webpack5": "^6.5.15",
    "@storybook/node-logger": "^6.5.15",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.15",
    "@storybook/testing-library": "^0.0.13",
    "apollo": "^2.34.0",
    "graphql": "~15.8.0",
    "msw": "^0.49.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
